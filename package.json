    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.55",
    "@langchain/openai": "^0.4.4",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "langchain": "^0.3.19",
    "openai": "^4.86.2",
    "pg": "^8.14.0",
    "readline-sync": "^1.4.10",
    "terraformer-wkt-parser": "^1.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
