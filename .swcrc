{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "@ai/*": ["./src/ai/*"],
      "@config/*": ["./src/config/*"],
      "@lib/*": ["./src/lib/*"],
      "@util/*": ["./src/util/*"]
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs"
  }
}
